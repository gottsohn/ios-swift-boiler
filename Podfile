# Uncomment this line to define a global platform for your project
platform :ios, '8.0'
# Uncomment this line if you're using Swift
use_frameworks!

def all_pods
    pod 'SwiftyJSON'
end

target 'IOSSwiftBoiler' do
	all_pods
end

target 'IOSSwiftBoilerTests' do
	all_pods
end

target 'IOSSwiftBoilerUITests' do
	all_pods
end

